  "homepage": "https://github.com/neekware/fullerstack.git",
  "scripts": {
    "ts-node": "./node_modules/.bin/ts-node -P ./tools/tsconfig.tools.json",
    "ng": "nx",
    "postinstall": "node ./decorate-angular-cli.js && ngcc --properties es2015 browser module main && yarn prisma:avidtrader-api:generate && husky install",
    "start": "ng serve",
    "start:dash": "ng serve avidtrader-client",
    "start:avidtrader-api": "ng serve avidtrader-api",
    "build": "ng build",
    "test": "ng test",
    "lint": "nx workspace-lint && ng lint",
    "e2e": "ng e2e",
    "<--------- [ affected related ] ------->": "affected related",
    "affected:apps": "nx affected:apps",
    "affected:libs": "nx affected:libs",
    "affected:build": "nx affected:build",
    "affected:e2e": "nx affected:e2e",
    "affected:test": "nx affected:test",
    "affected:lint": "nx affected:lint",
    "affected:dep-graph": "nx affected:dep-graph",
    "affected": "nx affected",
    "<--------- [ format related ] ------->": "format related",
    "format": "nx format:write",
    "format:write": "nx format:write",
    "format:check": "nx format:check",
    "format:all": "./node_modules/.bin/prettier --write \"./apps/**/*{.ts,.html,.js,.json}\" \"./libs/**/*{.ts,.html,.js,.json}\" \"./tools/**/*{.ts,.html,.js,.json}\"",
    "update": "nx migrate latest",
    "workspace-generator": "nx workspace-generator",
    "dep-graph": "nx dep-graph",
    "help": "nx help",
    "stats:build": "yarn build avidtrader-client --configuration=production --stats-json",
    "stats:show": "webpack-bundle-analyzer dist/apps/avidtrader-client/stats.json",
    "<--------- [ ci related ] ------->": "ci related",
    "nx:ci": "node ./node_modules/@nrwl/cli/bin/nx.js",
    "format:ci": "yarn nx:ci format:check",
    "lint:ci": "yarn nx:ci  run-many --target=lint --all --configuration=ci --parallel",
    "build:ci": "yarn nx:ci  run-many --target=build --all --configuration=ci --parallel",
    "test:ci": "yarn nx:ci  run-many --target=test --codeCoverage --all --configuration=ci --parallel",
    "e2e:ci": "yarn nx:ci  e2e avidtrader-client-e2e --configuration=ci --headless",
    "avidtrader-api:ci": "yarn nx:ci  serve avidtrader-api --configuration=ci",
    "dep-graph:main": " yarn nx:ci  dep-graph --file=graph/index.html --focus=avidtrader-client-e2e",
    "coverage:merge": "yarn ts-node ./tools/utils/coverageMerge.ts",
    "release": "yarn ts-node ./tools/utils/release.ts",
    "<--------- [ prisma related ] ------->": "prisma related",
    "prisma:avidtrader-api:migrate:create": "node ./node_modules/.bin/prisma migrate dev --create-only --schema=apps/avidtrader-api/src/prisma/schema.prisma",
    "prisma:avidtrader-api:migrate:apply": "node ./node_modules/.bin/prisma migrate deploy --schema=apps/avidtrader-api/src/prisma/schema.prisma",
    "prisma:avidtrader-api:migrate:dev": "node ./node_modules/.bin/prisma migrate dev --schema=apps/avidtrader-api/src/prisma/schema.prisma",
    "prisma:avidtrader-api:generate": "node ./node_modules/.bin/prisma generate --schema=apps/avidtrader-api/src/prisma/schema.prisma",
    "prisma:avidtrader-api:seed": "./node_modules/.bin/prisma db seed --schema=apps/avidtrader-api/src/prisma/schema.prisma",
    "i18n:extract": "node ./node_modules/.bin/ngx-translate-extract -i ./apps ./libs -o ./libs/agx-assets/src/lib/i18n/client/{en,fr,de,es,he,fa,zh-hans}.json -m _ --sort",
    "i18n:translate": "yarn ts-node ./tools/utils/i18n.ts",
    "gql:generate": "yarn ts-node ./tools/utils/gql.ts",
    "license": "yarn ts-node ./tools/utils/license.ts",
    "update:pkg": "yarn ts-node ./tools/utils/update.ts",
    "loc:generate": "yarn ts-node ./tools/utils/loc.ts",
    "prepare": "husky install",
    "serve:dist": "cd dist/apps/fullerstack; open http://localhost:8080; python3 -m http.server 8080",
    "<--------- [ test related ] ------->": "test related",
    "test:ipware": "yarn nx test nax-ipware"
  },
  "prisma": {
    "seed": "ts-node ./apps/avidtrader-api/src/prisma/seed.ts"
  },
  "importSort": {
    ".js, .ts": {
      "style": "module",
      "parser": "typescript"
    }
  },
